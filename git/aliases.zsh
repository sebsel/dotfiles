# === Git ===

alias gs="git status"
alias gl="git log"
alias gcom="git checkout master"
alias gaa="git add ."
alias gc="EDITOR=vim git commit -v"
alias gp="git push"
alias nah="git reset --hard && git clean -df"
alias git-hash="git rev-parse --short HEAD"
alias git-init="git init && gaa && git commit -q"

#=== Git Tower ===
alias tower="gittower"

